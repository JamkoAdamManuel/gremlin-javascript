{
  "name": "tinkergraph",
  "version": "1.0.0",
  "description": "A JavaScript in-memory graph structure/database",
  "main": "lib/index.js",
  "repository": "https://github.com/jbmusso/gremlin-javascript",
  "author": "Jean-Baptiste Musso <jbmusso+github@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build-watch": "npm run build -- --watch",
    "build": "npm run clean && tsc -p ./tsconfig.json",
    "clean": "rm -rf lib/*",
    "lint": "tslint src/**/*.ts",
    "test": "jest",
    "test-watch": "npm run test -- --watch"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/jest": "^22.1.4",
    "@types/node": "^9.4.6",
    "chai": "^4.1.2",
    "jest": "^22.4.2",
    "ts-jest": "^22.4.1",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
